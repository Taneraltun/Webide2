		<f:SimpleForm editable="true" layout="ResponsiveGridLayout" title="{i18n>GeneralData}" labelSpanXL="3"
			labelSpanL="3" labelSpanM="3" labelSpanS="12" adjustLabelSpan="false" emptySpanXL="4" emptySpanL="4" emptySpanM="4" emptySpanS="0"
			columnsXL="1" columnsL="1" columnsM="1" singleContainerFullSize="false">
			<f:content>
				<Label text="{i18n>Auart}" required="true"/>
				<Input id="inputAuart" width="auto" maxLength="4" showValueHelp="true" value="{generalModel>/Auart}">
					<layoutData>
						<l:GridData span="XL2 L3 M2 S5"/>
					</layoutData>
				</Input>
				<Label text="{i18n>Werks}"/>
				<Input id="inputWerks" width="auto" maxLength="4" showValueHelp="true" value="{generalModel>/Werks}">
					<layoutData>
						<l:GridData span="XL2 L3 M2 S12"/>
					</layoutData>
				</Input>
				<Label text="{i18n>Ilart}"/>
				<Input id="inputIlart" maxLength="3" showValueHelp="true" value="{generalModel>/Ilart}">
					<layoutData>
						<l:GridData span="XL2 L3 M2 S5"/>
					</layoutData>
				</Input>
				<Label text="{i18n>Vaplz}"/>
				<Input id="inputVaplz" maxLength="8" showValueHelp="true" value="{generalModel>/Vaplz}">
					<layoutData>
						<l:GridData span="XL4 L2 M4 S12"/>
					</layoutData>
				</Input>
				<Label text="{i18n>Ingpr}"/>
				<Input id="inputIngpr" maxLength="3" showValueHelp="true" value="{generalModel>/Ingpr}">
					<layoutData>
						<l:GridData span="XL2 L3 M2 S5"/>
					</layoutData>
				</Input>
				<Label text="{i18n>Tplnr}"/>
				<Input id="inputTplnr" maxLength="40" showValueHelp="true" value="{generalModel>/Tplnr}">
					<layoutData>
						<l:GridData span="XL4 L4 M4 S12"/>
					</layoutData>
				</Input>
				<Label text="{i18n>Equnr}"/>
				<Input id="inputEqunr" maxLength="18" showValueHelp="true" value="{generalModel>/Equnr}">
					<layoutData>
						<l:GridData span="XL4 L4 M4 S12"/>
					</layoutData>
				</Input>
				<Label text="{i18n>Stat}"/>
				<Input id="inputStat" maxLength="5" showValueHelp="true" value="{generalModel>/Stat}">
					<layoutData>
						<l:GridData span="XL2 L3 M2 S5"/>
					</layoutData>
				</Input>
				<Label text="{i18n>Kostl}"/>
				<Input id="inputKostl" maxLength="10" showValueHelp="true" value="{generalModel>/Kostl}">
					<layoutData>
						<l:GridData span="XL4 L4 M4 S12"/>
					</layoutData>
				</Input>
				<Label text="{i18n>Zzistkonclk}"/>
				<Input id="inputZzistkonclk" maxLength="3" showValueHelp="true" value="{generalModel>/Zzistkonclk}">
					<layoutData>
						<l:GridData span="XL2 L3 M2 S5"/>
					</layoutData>
				</Input>
				<Label text="{i18n>Priok}"/>
				<Input id="inputPriok" maxLength="1" showValueHelp="true" value="{generalModel>/Priok}">
					<layoutData>
						<l:GridData span="XL2 L3 M2 S5"/>
					</layoutData>
				</Input>
				<Label text="{i18n>Anlzu}"/>
				<Input id="inputAnlzu" maxLength="1" showValueHelp="true" value="{generalModel>/Anlzu}">
					<layoutData>
						<l:GridData span="XL2 L3 M2 S5"/>
					</layoutData>
				</Input>
				<Label text="{i18n>Msaus}"/>
				<Input id="inputMsaus" maxLength="1" showValueHelp="true" value="{generalModel>/Msaus}">
					<layoutData>
						<l:GridData span="XL1 L1 M1 S1"/>
					</layoutData>
				</Input>
				<Label text="{i18n>Zzdgisaciklm}"/>
				<Input id="inputZzdgisaciklm" maxLength="30" value="{generalModel>/Zzdgisaciklm}">
					<layoutData>
						<l:GridData span="XL4 L4 M4 S12"/>
					</layoutData>
				</Input>
				<Label text="{i18n>Plnnr}"/>
				<Input id="inputPlnnr" maxLength="8" value="{generalModel>/Plnnr}">
					<layoutData>
						<l:GridData span="XL4 L4 M4 S12"/>
					</layoutData>
				</Input>
				<Label text="{i18n>Zzhsponcelik}"/>
				<Input id="inputZzhsponcelik" type="Number" maxLength="8" value="{generalModel>/Zzhsponcelik}">
					<layoutData>
						<l:GridData span="XL4 L4 M4 S12"/>
					</layoutData>
				</Input>
				<Label text="{i18n>Zgaranti}"/>
				<Input id="inputZgaranti" maxLength="1" value="{generalModel>/Zgaranti}">
					<layoutData>
						<l:GridData span="XL2 L3 M2 S5"/>
					</layoutData>
				</Input>
				<Label text="{i18n>Erdat}"/>
				<DatePicker id="dpErdat" visible="true"
					value="{path:'generalModel>/Erdat', type:'sap.ui.model.type.Date', formatOptions: {style: 'short', strictParsing: true}}">
					<layoutData>
						<l:GridData span="XL2 L2 M2 S12"/>
					</layoutData>
				</DatePicker>
				<Label text="{i18n>Aedat}"/>
				<DatePicker id="dpAedat" visible="true"
					value="{path:'generalModel>/Aedat', type:'sap.ui.model.type.Date', formatOptions: {style: 'short', strictParsing: true}}">
					<layoutData>
						<l:GridData span="XL2 L2 M2 S12"/>
					</layoutData>
				</DatePicker>
				<Label text="{i18n>Oteil}"/>
				<Input id="inputOteil" maxLength="4" value="{generalModel>/Oteil}">
					<layoutData>
						<l:GridData span="XL2 L3 M2 S5"/>
					</layoutData>
				</Input>
				<Label text="{i18n>Zzarz}"/>
				<Input id="inputZzarz" maxLength="3" value="{generalModel>/Zzarz}">
					<layoutData>
						<l:GridData span="XL2 L3 M2 S5"/>
					</layoutData>
				</Input>
				<Label text="{i18n>Zzsbp}"/>
				<Input id="inputZzsbp" maxLength="4" value="{generalModel>/Zzsbp}">
					<layoutData>
						<l:GridData span="XL2 L3 M2 S5"/>
					</layoutData>
				</Input>
				<Label text="{i18n>Zzczm}"/>
				<Input id="inputZzczm" maxLength="5" value="{generalModel>/Zzczm}">
					<layoutData>
						<l:GridData span="XL2 L3 M2 S5"/>
					</layoutData>
				</Input>
			</f:content>
		</f:SimpleForm>
	</VBox>
</core:FragmentDefinition>